{
  "name": "@labkey/api",
  "version": "0.0.1",
  "description": "JavaScript client API for LabKey Server",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.js && npm run build-rollup",
    "build-rollup": "rollup -c",
    "clean": "rimraf dist",
    "setup": "npm install",
    "test": "jest --no-cache --config test/configs/jest.json",
    "test-coverage": "jest --no-cache --config test/configs/jest-coverage.json"
  },
  "keywords": [
    "labkey",
    "javascript"
  ],
  "main": "dist/labkey-api-js-core.cjs.js",
  "module": "dist/labkey-api-js-core.es.js",
  "types": "dist/labkey.d.ts",
  "author": {
    "name": "Nick Kerr",
    "email": "nicka@labkey.com",
    "url": "https://www.labkey.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LabKey/labkey-api-js.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "19.2.4",
    "app-root-path": "2.0.1",
    "jest": "19.0.2",
    "rimraf": "2.6.2",
    "rollup": "0.59.1",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-typescript2": "0.14.0",
    "sinon": "2.3.2",
    "ts-loader": "3.5.0",
    "typescript": "2.8.3",
    "webpack": "3.11.0"
  }
}
